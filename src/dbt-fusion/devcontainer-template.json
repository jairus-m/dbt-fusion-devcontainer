{
    "name": "dbt Fusion",
    "version": "1.0.2",
    "description": "A development container for dbt-fusion projects with additional tools and configurations.",
    "documentationURL": "https://github.com/jairus-m/dbt-fusion-devcontainer",
    "licenseURL": "https://github.com/jairus-m/dbt-fusion-devcontainer/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Debian OS version (use bullseye on local arm64/Apple Silicon, bookworm elsewhere)",
            "proposals": [
                "bullseye",
                "bookworm"
            ],
            "default": "bullseye"
        },
        "pythonVersion": {
            "type": "string",
            "description": "Python version to use",
            "proposals": [
                "3.12",
                "3.11",
                "3.10"
            ],
            "default": "3.12"
        }
    }
} 